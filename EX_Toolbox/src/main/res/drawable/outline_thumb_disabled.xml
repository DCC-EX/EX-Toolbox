<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/transparent"/>
            <stroke
                android:width="@dimen/buttonStroke_outline"
                android:color="@color/buttonDisabledStroke_outline"
                />
            <size
                android:width="20dp"
                android:height="40dp" />
            <corners android:radius="4dp" />

        </shape>
    </item>

    <item
        android:top="@dimen/buttonSelectedInsideStrokePadding_outline" android:left="@dimen/buttonSelectedInsideStrokePadding_outline"
        android:right="@dimen/buttonSelectedInsideStrokePadding_outline" android:bottom="@dimen/buttonSelectedInsideStrokePadding_outline">

        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="@dimen/buttonStroke_outline"
                android:color="@color/buttonDisabledStroke_outline"
                />
            <size
                android:width="10dp"
                android:height="30dp" />
            <corners android:radius="2dp" />

        </shape>
    </item>
</layer-list>
