                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:padding="2sp"
                                android:text="@string/app_name_speed_matching"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="2dp"
                            android:layout_marginTop="8sp"
