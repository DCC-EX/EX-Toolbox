                                    android:layout_marginRight="8sp"
                                    android:inputType="number"
                                    android:maxLength="5"
                                    android:paddingRight="8sp"
                                    android:shadowColor="#FFFFFFFF"
                                    android:shadowDx="1"
                                    android:shadowDy="1"
                                    android:shadowRadius="0.3"
                                    android:singleLine="true"
