                                style="?attr/floating_text_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="1sp"
                                android:layout_weight=".05"
                                android:text="H"
                                android:textSize="12sp"
                                tools:ignore="HardcodedText" />

