                                android:layout_marginRight="8sp"
                                android:layout_weight=".2"
                                android:inputType="number"
                                android:maxLength="5"
                                android:paddingRight="8sp"
                                android:shadowColor="#FFFFFFFF"
                                android:shadowDx="1"
                                android:shadowDy="1"
                                android:shadowRadius="0.3"
                                android:singleLine="true"
                                android:text=""
                                tools:ignore="Autofill,LabelFor,RtlHardcoded,RtlSymmetry" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/dexc_DCCEXtrack3layout"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0sp"
                            android:background="#00000000"
                            android:orientation="horizontal"
                            android:layout_weight=".25"
                            android:weightSum="1" >

                            <TextView
                                style="?attr/floating_text_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="1sp"
                                android:layout_weight=".05"
                                android:text="D"
                                android:textSize="12sp"
                                tools:ignore="HardcodedText" />

                            <Spinner
                                android:id="@+id/dexc_track_type_3_list"
                                android:layout_width="0sp"
                                android:layout_height="wrap_content"
                                android:ellipsize="none"
                                android:paddingLeft="0dp"
                                android:paddingRight="0dp"
                                android:paddingBottom="2dp"
                                android:text=""
                                android:layout_weight=".45"
                                tools:ignore="RtlHardcoded" />

                            <EditText
                                android:id="@+id/dexc_track_3_value"
                                style="?attr/edit_label_style"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="8sp"
                                android:layout_marginRight="8sp"
                                android:layout_weight=".2"
                                android:inputType="number"
                                android:maxLength="5"
                                android:paddingRight="8sp"
                                android:shadowColor="#FFFFFFFF"
                                android:shadowDx="1"
                                android:shadowDy="1"
                                android:shadowRadius="0.3"
                                android:singleLine="true"
                                android:text=""
                                tools:ignore="Autofill,LabelFor,RtlHardcoded,RtlSymmetry" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/dexc_DCCEXtrack4layout"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0sp"
                            android:background="#00000000"
                            android:orientation="horizontal"
                            android:layout_weight=".25"
                            android:weightSum="1" >

                            <TextView
                                style="?attr/floating_text_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="1sp"
                                android:layout_weight=".05"
                                android:text="E"
                                android:textSize="12sp"
                                tools:ignore="HardcodedText" />

                            <Spinner
                                android:id="@+id/dexc_track_type_4_list"
                                android:layout_width="0sp"
                                android:layout_height="wrap_content"
                                android:ellipsize="none"
                                android:paddingLeft="0dp"
                                android:paddingRight="0dp"
                                android:paddingBottom="2dp"
                                android:text=""
                                android:layout_weight=".45"
                                tools:ignore="RtlHardcoded" />

                            <EditText
                                android:id="@+id/dexc_track_4_value"
                                style="?attr/edit_label_style"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="8sp"
                                android:layout_marginRight="8sp"
                                android:layout_weight=".2"
                                android:inputType="number"
                                android:maxLength="5"
                                android:paddingRight="8sp"
                                android:shadowColor="#FFFFFFFF"
                                android:shadowDx="1"
                                android:shadowDy="1"
                                android:shadowRadius="0.3"
                                android:singleLine="true"
                                android:text=""
                                tools:ignore="Autofill,LabelFor,RtlHardcoded,RtlSymmetry" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/dexc_DCCEXtrack5layout"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0sp"
                            android:background="#00000000"
                            android:orientation="horizontal"
                            android:layout_weight=".25"
                            android:weightSum="1" >

                            <TextView
                                style="?attr/floating_text_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="1sp"
                                android:layout_weight=".05"
                                android:text="F"
                                android:textSize="12sp"
                                tools:ignore="HardcodedText" />

                            <Spinner
                                android:id="@+id/dexc_track_type_5_list"
                                android:layout_width="0sp"
                                android:layout_height="wrap_content"
                                android:ellipsize="none"
                                android:paddingLeft="0dp"
                                android:paddingRight="0dp"
                                android:paddingBottom="2dp"
                                android:text=""
                                android:layout_weight=".45"
                                tools:ignore="RtlHardcoded" />

                            <EditText
                                android:id="@+id/dexc_track_5_value"
                                style="?attr/edit_label_style"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="8sp"
                                android:layout_marginRight="8sp"
                                android:layout_weight=".2"
                                android:inputType="number"
                                android:maxLength="5"
                                android:paddingRight="8sp"
                                android:shadowColor="#FFFFFFFF"
                                android:shadowDx="1"
                                android:shadowDy="1"
                                android:shadowRadius="0.3"
                                android:singleLine="true"
                                android:text=""
                                tools:ignore="Autofill,LabelFor,RtlHardcoded,RtlSymmetry" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/dexc_DCCEXtrack6layout"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0sp"
                            android:background="#00000000"
                            android:orientation="horizontal"
                            android:layout_weight=".25"
                            android:weightSum="1" >

                            <TextView
                                style="?attr/floating_text_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="1sp"
                                android:layout_weight=".05"
