                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1.0" >
                    <RelativeLayout
                        android:id="@+id/loco_cell_T"
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight=".73" >
                        <Button
                            android:id="@+id/button_select_loco_T"
                            style="@style/normal_button_style"
                            android:layout_width="fill_parent"
                            android:layout_alignParentRight="true"
                            android:text="@string/none" />
                        <TextView
                            android:id="@+id/loco_label_T"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="6dp"
                            android:paddingTop="2dp"
                            android:shadowColor="#FFFFFFFF"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="1"
                            android:text="@string/loco"
                            android:textColor="#666666" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/speed_cell_T"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight=".27" >
                        <TextView
                            android:id="@+id/speed_label_T"
                            style="@style/floating_text_style"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:paddingLeft="2dp"
                            android:paddingTop="2dp"
                            android:text="@string/speed" />
                        <TextView
                            android:id="@+id/speed_value_label_T"
                            style="@style/floating_text_style"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:enabled="false"
                            android:text="0"
                            android:textSize="36dp" />
                        <TextView
                            android:id="@+id/volume_indicator_T"
                            style="@style/floating_text_style"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/speed_value_label_T"
                            android:layout_alignParentLeft="true"
                            android:enabled="false"
                            android:text="@string/volume_indicator" />
                    </RelativeLayout>
                </LinearLayout>
                <TableLayout
