                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:paddingRight="20dp"
                        android:stretchColumns="0,1,2,3" >
                        <TableRow>
                            <Button
                                style="@style/fn_button_style"
                                android:text="Light" />
                            <Button
                                style="@style/fn_button_style"
                                android:text="Bell" />
                            <Button
                                style="@style/fn_button_style"
                                android:text="Horn" />
                            <Button
